            <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                       name="type"
                       a:defaultValue="mainEntry">
               <a:documentation/>
               <text/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="sense">
