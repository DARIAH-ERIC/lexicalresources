         <ref name="macro.paraContent"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="TEILex0-p-abstractModel-structure-p-in-ab-or-p-constraint-report-5">
            <rule context="tei:p">
