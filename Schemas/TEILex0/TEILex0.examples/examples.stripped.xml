         <!--TODO: lbl vs. num -\- I'd prefer to introduce lbl for ALL labels, and type accordingly -->
         <entry xml:id="ED.aid" xml:lang="en">
            <form type="lemma">
               <orth>aid</orth>
               <pron>/ed/</pron>
            </form>
            <gramGrp>
               <gram type="pos">noun</gram>
            </gramGrp>
            <sense xml:id="ED.aid.n.1" n="1">
               <lbl type="num">1.</lbl>
               <def>help, especially money, food or other gifts given to people living in difficult
                  conditions</def>
               <cit type="example">
                  <quote>aid to the earth-quake zone an aid worker</quote>
               </cit>
               <note>(NOTE: This meaning of aid has no plural.)</note>
               <entry type="relatedEntry" xml:id="ED.in_aid_of" xml:lang="en">
                  <form type="lemma">
                     <orth>in aid of</orth>
                  </form>
                  <sense xml:id="ED.in_aid_of.1">
                     <def>in order to help</def>
                     <cit type="example">
                        <quote>We give money in aid of the Red Cross.</quote>
                     </cit>
                     <cit type="example">
                        <quote>They are collecting money in aid of refugees.</quote>
                     </cit>
                  </sense>
               </entry>
            </sense>
            <sense xml:id="ED.aid.n.2" n="2">
               <lbl type="num">2.</lbl>
               <def>thing which helps you to do something</def>
               <cit type="example">
                  <quote>kitchen aids</quote>
               </cit>
            </sense>
            <sense xml:id="ED.aid.v.1" n="3">
               <gramGrp>
                  <gram type="pos">verb</gram>
               </gramGrp>
               <sense xml:id="ED.aid.v.1.1" n="3.1">
                  <lbl type="n">1.</lbl>
                  <def>to help something to happen</def>
               </sense>
               <sense xml:id="ED.aid.v.1.2" n="3.2">
                  <lbl>2.</lbl>
                  <def>to help someone</def>
               </sense>
            </sense>
         </entry>

