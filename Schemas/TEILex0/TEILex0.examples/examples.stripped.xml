         <entry type="mainEntry" xml:lang="en" xml:id="arrest">
            <form type="lemma">
               <orth>arrest</orth>
               <pron>/ə rest/</pron>
            </form>
            <sense xml:id="arrest_verb_sense">
               <gramGrp>
                  <gram type="pos">verb</gram>
               </gramGrp>
               <usg type="hint">(of the police)</usg>
               <def>to catch and hold someone who has broken the law</def>
               <cit type="example">
                  <quote>The police arrested two men and took them to the police station</quote>
               </cit>
               <pc>.</pc>
               <cit type="example">
                  <quote>He ended up getting arrested as he tried to leave the country</quote>
               </cit>
               <pc>.</pc>
               <cit type="example">
                  <quote>She was arrested for stealing</quote>
               </cit>
            </sense>
            <pc>.</pc>
            <sense xml:id="arrest_noun_sense">
               <gramGrp>
                  <gram type="pos">noun</gram>
               </gramGrp>
               <def>the act of holding someone for breaking the law</def>
               <cit type="example">
                  <quote>The police made several arrests at the demonstration.</quote>
               </cit>
               <entry type="relatedEntry" xml:lang="en" xml:id="arrest_related.1">
                  <form>under arrest</form>
                  <sense xml:id="arrest_related.1_sense.1">
                     <def>held by the police</def>
                     <pc>,</pc>
                     <cit type="example">
                        <quote>After the fight, three people were under arrest.</quote>
                     </cit>
                  </sense>
               </entry>
            </sense>
         </entry>
