         <!--organize entries alphabetically-->
         <entry xml:id="Eingang" xml:lang="de">
            <form type="lemma"><orth>Eingang</orth></form>
            <gramGrp><gen>m.</gen></gramGrp>
            <etym>
               <cit type="etymon">
                  <lang expand="Mittel Hoch Deutsch">mhd.</lang>
                  <form><orth xml:lang="gmh">īnganc</orth></form>
               </cit>
               <cit type="cognate">
                  <lang expand="Neuniederländisch">nnl.</lang>
                  <form><orth>ingang</orth></form>
               </cit>
               <cit type="cognate">
                  <lang expand="Dänisch">dän.</lang>
                  <form><orth xml:lang="da">indgang</orth></form>
               </cit>
               <cit type="cognate">
                  <lang expand="Schwedisch">schwed.</lang>
                  <form><orth xml:lang="sv">ingång</orth></form>
               </cit>
               <seg type="desc">Lehnübersetzung des</seg>
               <cit type="etymon">
                  <lang expand="Latein">lat.</lang>
                  <form><orth xml:lang="la">introitus</orth></form>
               </cit>
               <seg type="desc">Aus dem ‘Hineingehen’ als Handlung ist die ‘Stelle, an der man ins
                  Haus, in den Saal geht’ geworden, neuerdings auch die ‘Gesamtheit der
                  eingegangenen Geschäftssachen, Mannschaften’ usw. Vgl. <xr type="crossReference">
                     <ref>Zugang</ref>
                  </xr>. <bibl>(Kluge, 1975) p. 159</bibl></seg>
            </etym>
            <entry xml:id="Eingang2" xml:lang="de">
               <etym type="inheritance">
                  <cit type="etymon">
                     <lang expand="Middle High German">mhd.</lang>
                     <form><orth xml:lang="gmh">īnganc</orth></form>
                  </cit>
                  <!-- cognates here -->
                  <etym type="borrowing" subtype="calque">
                     <seg type="desc">Lehnübersetzung des</seg>
                     <cit type="etymon">
                        <lang expand="Latein">lat.</lang>
                        <form><orth xml:lang="la">introitus</orth></form>
                     </cit>
                     <seg type="desc">Aus dem ‘Hineingehen’ als Handlung ist die ‘Stelle, an der man
                        ins Haus, in den Saal geht’ geworden, neuerdings auch die ‘Gesamtheit der
                        eingegangenen Geschäftssachen, Mannschaften’ usw. Vgl. <xr
                           type="crossReference">
                           <ref>Zugang</ref>
                        </xr>. <bibl>(Kluge, 1975) p. 159</bibl></seg>
                  </etym>
               </etym>

            </entry>

