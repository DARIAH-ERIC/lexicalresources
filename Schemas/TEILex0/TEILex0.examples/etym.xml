         <!--suffixal derivation-->
         <entry xml:lang="pt">
            <form type="lemma">
               <orth xml:lang="pt">húmanal</orth>
               <pron xml:lang="pt" notation="ipa">umɐnáł</pron>
            </form>
            <gramGrp>
               <pos>adj.</pos>
               <gen>m.</gen>
               <lbl>e</lbl>
               <gen>f.</gen>
            </gramGrp>
            <etym type="suffixalDerivation">
               <pc>(</pc>
               <seg type="desc">De</seg>
               <cit type="etymon">
                  <form>
                     <orth xml:lang="pt">humano</orth>
                  </form>
               </cit>
               <pc>+</pc>
               <cit type="etymon">
                  <gramGrp>
                     <gram>suf.</gram>
                  </gramGrp>
                  <form>
                     <orth extent="suff" xml:lang="pt">-al</orth>
                  </form>
               </cit>
               <pc>)</pc>
            </etym>
