                    </def>
                    <cit type="translation">
                        <quote xml:lang="pl">korpus referencyjny (<lang value="pl"
                            >pl</lang>)</quote>
                        <quote xml:lang="en">benchmark corpus (<lang value="en">en</lang>)</quote>
                    </cit>
                </sense>
