         <entry xml:lang="pt" xml:id="anel">
            <form type="lemma">
               <orth>anel</orth>
            </form>
            <gramGrp>
               <pos>nome</pos>
               <gen>masculino</gen>
            </gramGrp>
            <sense>
               <def>pequena argola, de ouro, prata ou outro material, que se usa no dedo</def>
               <pc>;</pc></sense>
            <sense>
               <def>cada uma das peças de uma corrente; elo</def>
               <pc>;</pc>
               <def>arco</def>
               <pc>;</pc>
               <def>espiral de cabelo frisado ou encaracolado; caracol</def>
            </sense>
            <etym>Do latim anellu-, "pequeno anel"</etym>
         </entry>
         <entry xml:lang="pt" xml:id="garagem">
