  <text>
      <body>
         <entry xml:id="lusco-fusco">
            <form type="lemma">
               <orth>lusco-fusco</orth>
               <pron notation="ipa">luʃkuˈfuʃku</pron>
               <syll>lus.co-fus.co</syll>
            </form>
            <gramGrp>
               <pos>Nome</pos>
               <gen>Masculino</gen>
               <number>Singular</number>
            </gramGrp>
            <sense n="1"> 
               <num>1.</num>
               <def>
                  <gloss>o anoitecer</gloss>; 
                  <gloss>crepúsculo vespertino</gloss>
               </def>
            </sense>
            <sense n="2"> 
               <num>2.</num>
               <def>
                  <gloss>penumbra;</gloss> 
                  <gloss>pouca luz</gloss>
               </def>
            </sense>
         </entry>
         <entry xml:id="bizarro">
            <form type="lemma">
               <orth>bizarro</orth>
               <pron notation="ipa">biˈzaʀu</pron>
               <syll>bi.zar.ro</syll>
            </form>
            <etym>do 
               <lang>Vasconço</lang> 
               <mentioned>bizarra</mentioned>
               <gloss>barba cerrada</gloss>
            </etym>
            <etym>pelo
               <lang>castelhano</lang>
               <mentioned>bizarro</mentioned>
               <gloss>valente</gloss>
            </etym>
            <gramGrp>
               <pos>Adjetivo</pos>
            </gramGrp>
            <sense n="1">
               <num>1.</num>
               <def>
                  <gloss>esquisito</gloss>; 
                  <gloss>excêntrico</gloss>; 
                  <gloss>insólito</gloss>
               </def>
            </sense>
            <sense n="2">
               <num>2.</num>
               <def>
                  <gloss>gentil</gloss>; 
                  <gloss>generoso</gloss>
               </def>
            </sense>
            <sense n="3">
               <num>3.</num>
               <def>
                  <gloss>nobre</gloss>; 
                  <gloss>valente</gloss>
               </def>
            </sense>
            <sense n="4">
               <num>4.</num>
               <def>
                  <gloss>arrogante</gloss>; 
                  <gloss>fanfarrão</gloss>
               </def>
            </sense>
         </entry>
         <entry xml:id="lhano">
            <form type="lemma">
               <orth>lhano</orth>
               <pron notation="ipa">ˈʎɐnu</pron>
               <syll>lha.no</syll>
            </form>
            <etym>do 
               <lang>Castelhano</lang> 
               <mentioned>llano</mentioned>
               <gloss>idem</gloss>
            </etym>
            <gramGrp>
               <pos>Adjetivo</pos>
            </gramGrp>
            <sense n="1">
               <num>1.</num>
               <def>
                  <gloss>franco</gloss>; 
                  <gloss>sincero</gloss>
               </def>
            </sense>
            <sense n="2">
               <num>2.</num>
               <def>
                  <gloss>afável</gloss>; 
                  <gloss>amável</gloss>
               </def>
            </sense>
            <sense n="3">
               <num>3.</num>
               <def>
                  <gloss>simples</gloss>; 
                  <gloss>despretensioso</gloss>
               </def>
            </sense>
            </entry>
